{
    "root": "./docs",
    "plugins": [
        "fontsettings",
        "sharing",
        "lunr",
        "search",
        "highlight",
        "livereload",
        "theme-corezoid",
        "-sharing"
    ],
    "variables": {
        "themeCorezoid": {
            "langs": [
                "ru",
                "en"
            ]
        }
    },
    "pdf": {
        "toc": true,
        "pageNumbers": true,
        "fontSize": 10,
        "paperSize": "letter",
        "margin": {
            "right": 72,
            "left": 72,
            "top": 72,
            "bottom": 72
        }
    },
    "sidebar": false
}